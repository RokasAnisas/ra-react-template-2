{
  "name": "ra-react-template-2",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Rokas Anisas",
    "email": "rokas.anisas@gmail.com"
  },
  "packageManager": "yarn@3.2.0",
  "engineStrict": true,
  "engines": {
    "node": ">=16.13.0",
    "npm": "please-use-yarn",
    "yarn": ">=3.2.0"
  },
  "scripts": {
    "start": "vite --host",
    "build": "yarn lint:ts && vite build",
    "preview": "vite preview",
    "lint:files": "npx @ls-lint/ls-lint",
    "lint:code": "eslint \"src/**/*.{js,jsx,json,ts,tsx}\"",
    "lint:ts": "yarn tsc --noEmit --skipLibCheck",
    "lint": "yarn lint:code && yarn lint:ts && yarn lint:files",
    "lint:fix": "prettier --write \"src/**/*.{js,jsx,json,md,mdx,ts,tsx}\" && yarn lint:code --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "generate": "motem templates",
    "update": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.6",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.4",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@playwright/test": "^1.27.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "browserslist": "^4.21.4",
    "c8": "^7.12.0",
    "eslint": "^8.25.0",
    "eslint-config-ra-react": "^1.2.1",
    "jsdom": "^20.0.1",
    "motem": "^1.0.4",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  }
}
