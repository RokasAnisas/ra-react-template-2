{
  "name": "ra-react-template-2",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Rokas Anisas",
    "email": "rokas.anisas@gmail.com"
  },
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "start": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:files": "npx @ls-lint/ls-lint",
    "lint:ts": "eslint \"src/**/*.{js,jsx,json,ts,tsx}\" && yarn tsc --noEmit --skipLibCheck",
    "lint": "yarn lint:ts && yarn lint:files",
    "lint:fix": "prettier --write \"src/**/*.{js,jsx,json,md,mdx,ts,tsx}\" && eslint --fix \"src/**/*.{js,jsx,json,ts,tsx}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "generate": "motem templates",
    "update": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.1",
    "react-redux": "^8.0.2",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@playwright/test": "^1.22.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "browserslist": "^4.20.4",
    "c8": "^7.11.3",
    "eslint": "^8.19.0",
    "eslint-config-ra-react": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^19.0.0",
    "motem": "^1.0.4",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.14.2"
  }
}
